var ados = ados || {};
ados.run = ados.run || [];
/* load placement for account: linuxnewmedia, site: Distrowatch, size: 728x90 - Leaderboard, zone: Leaderboard*/
ados.run.push(function() {
ados_addInlinePlacement(4669, 20978, 4).setZone(16022).setClickUrl('-optional-click-macro-').loadInline();
});</script>
